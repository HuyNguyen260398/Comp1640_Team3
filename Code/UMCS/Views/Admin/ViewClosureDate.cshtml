@model IEnumerable<UMCS.Models.ClosureDate>

@{
    ViewBag.Title = "ViewClosureDate";
    Layout = "~/Views/Layout/Admin.cshtml";
}

<div class="container-fluid">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="sparkline13-list">
                <div class="sparkline13-graph">
                    <div class="datatable-dashv1-list custom-datatable-overright">
                        <table class="table" style="border: 2px solid #006DF0">
                            <thead>
                                <tr>
                                    <th style="background-color: #006DF0; color:white; text-align:center">
                                        @Html.DisplayNameFor(model => model.AcademicYear)
                                    </th>

                                    <th style="background-color: #006DF0; color:white; text-align:center">
                                        @Html.DisplayNameFor(model => model.ClosureDate1)
                                    </th>

                                    <th style="background-color: #006DF0; color:white; text-align:center">
                                        @Html.DisplayNameFor(model => model.FinalClosureDate)
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td style="text-align:center">
                                            @Html.DisplayFor(modelItem => item.AcademicYear)
                                        </td>

                                        <td style="text-align:center">
                                            @Html.DisplayFor(modelItem => item.ClosureDate1)
                                        </td>

                                        <td style="text-align:center">
                                            @Html.DisplayFor(modelItem => item.FinalClosureDate)
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div style="height:110px"></div>
